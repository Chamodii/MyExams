<%include partials/headerStudent.ejs%>

        <div class="col-sm-6">
            <h2 class="mt-4">Online Module Registration</h2>
            <p>To register for modules, please select the semester.</p>
            <form action="/reg/<%=user.username%>" method="post">
                <div class="form-group">
                    <select name="semester" data-style="btn-info" class="selectpicker" required>
                        <option value=1>Semester 1</option>
                        <option value=2>Semester 2</option>
                        <option value=3>Semester 3</option>
                        <option value=4>Semester 4</option>
                        <option value=5>Semester 5</option>
                        <option value=6>Semester 6</option>
                        <option value=7>Semester 7</option>
                        <option value=8>Semester 8</option>
                    </select>
                </div>
                <button type="submit" class="btn btn-success">Submit</button>
            </form>

            <br></br>
            

        </div>

