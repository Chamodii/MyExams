<%include partials/headerStudent.ejs%>

        <div class="col-sm-6">
            <h2 class="mt-4">Examination Division</h2>
            <form action="/editProfile/<%=user._id%>" method="post">
                <div  class="form-group">
                    <p>Full name : <%=user.firstName%> <%=user.lastName%></p>
                </div>
                <div class="form-group">
                    <p>ID : <%=user.username%></p>
                </div>
                <div class="form-group">
                    <p>Department : <%=user.department%></p>
                </div>
                <div class="form-group">
                    <p>Degree/Course : <%=user.degree%></p>
                </div>
                <div class="form-group">
                    <p>Batch : <%=user.batch%></p>
                </div>
                <div class="form-group">
                    <p>Faculty : <%=user.faculty%></p>
                </div>
                <div class="form-group">
                    <p>NIC : <%=user.NIC%></p>
                </div>
                <div class="form-group">
                    <input type="email" value="<%=user.email%>" name="email" placeholder="Email">
                </div>
                <button type="submit" class="btn btn-success">Submit</button>
            </form>




        </div>

        <div class="col-sm-3 sidebar content-box" style="background-color:#eee;">
            <ul class="nav">
                <li>
                    <h5 class="mt-3" id="upcoming_events">Upcoming Events<span onclick="document.getElementById('notice_bar1').style.display='none'" class="close" data-dismiss="alert" position="left">&times;</button></h5>
                    <p id="para">The Examinations Division has responsibility for making arrangements for the administration of all public examination</p>
                </li>
                <li>
                    <hr></hr>
                    <h5 class="mt-3" id="notices">Notices<span onclick="document.getElementById('notice_bar1').style.display='none'" class="close" data-dismiss="alert" position="left">&times;</button></h5>
                    <p id="para">The Examinations Division has responsibility for making arrangements for the administration of all public examination  </p>
                </li>
                <li>
                    <hr></hr>
                    <h6 id="related"> Related Services</h6>
                    <ul style="list-style-type:none">
                        <li style="font-size:12px;"><a href="#">Results</a><li>
                        <li style="font-size:12px;"><a href="#">Module Registration</a></li>
                    </ul>
                </li>
            </ul>
        </div>

    </div>
</div>
<!-- Footer -->
<%include partials/footer.ejs%>